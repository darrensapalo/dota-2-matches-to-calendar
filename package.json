{
  "name": "dota-2-games-to-google-calendar",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc",
    "tar-credentials": "tar cvf secrets.tar credentials.json token.json api-project.json",
    "travis-encrypt": "travis encrypt-file secrets.tar --com",
    "re-encrypt": "npm run tar-credentials; npm run travis-encrypt",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts --experimental-modules",
    "prod": "tsc && node ./dist/index.js"
  },
  "dependencies": {
    "@akanass/rx-http-request": "^3.1.0",
    "@types/express": "^4.16.1",
    "@types/gapi": "0.0.36",
    "@types/request": "^2.48.1",
    "express": "^4.16.4",
    "googleapis": "^42.0.0",
    "marked": "^0.7.0",
    "moment": "^2.24.0",
    "readline": "^1.3.0",
    "request": "^2.88.0",
    "rxjs": "^6.4.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "ts-node-dev": "^1.0.0-pre.32"
  }
}
